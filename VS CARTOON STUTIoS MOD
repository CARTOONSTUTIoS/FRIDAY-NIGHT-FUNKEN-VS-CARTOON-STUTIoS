			<script>
	// <![CDATA[
		var medals = [{"medal_id":60960,"medal_name":"Friday Night Funker","medal_description":"Start the game","medal_difficulty":1,"medal_icon":"https:\/\/apifiles.ngfiles.com\/medals\/51000\/51023\/60960_fridaynightfunker.jpg?1604201951","is_secret":0,"approved":true,"denied":false,"value":5},{"medal_id":60961,"medal_name":"SPOOKY TIME","medal_description":"Beat Week 1 in Story mode and unlock Week 2","medal_difficulty":2,"medal_icon":"https:\/\/apifiles.ngfiles.com\/medals\/51000\/51023\/60961_spookytime.png?1604217811","is_secret":0,"approved":true,"denied":false,"value":10},{"medal_id":61034,"medal_name":"Just like the game","medal_description":"Funk on a Friday (real time)","medal_difficulty":3,"medal_icon":"https:\/\/apifiles.ngfiles.com\/medals\/51000\/51023\/61034_justlikethegame.jpg?1604648857","is_secret":0,"approved":true,"denied":false,"value":25}];

		function getMedal(id) {
			for(var i=0; i<medals.length; i++) {
				if (medals[i].medal_id == id) return medals[i];
			}
			return null;
		}

		// this function has to be globally accessible by flash's externalinterface calls
		function sandboxEvent(event, params)
		{
			// this timer is used to prevent flash from freezing from events that don't return instantly (such as alerts)
			// this is done using an anonymous function that also gets a jquery reference

						setTimeout(function($)
			{
				var _this = this;

									this.trace = function(msg) {}
				
				this.unlockMedal = function(params) {
					var medal_id = params.data.medal_id;
					var medal = getMedal(medal_id);

					if (medal) {
											} else {
						_this.trace("WARNING:\n    The medal you unlocked does not appear to exist (medal id# "+medal_id+").\n    If you recently added this medal, you may need to reload this page.");
					}

					if ($('#medal_'+medal_id)) {
						$('#medal_click_'+medal_id).hide();
						var medal = getMedal(medal_id);
						if (medal.is_secret) {
							$('#medal_'+medal_id).css('background-image', 'url(' + medal.medal_icon + ')');
							if (medal.medal_name) $('#medal_name_'+medal_id).html(medal.medal_name);
							if (medal.medal_description) $('#medal_description_'+medal_id).html(medal.medal_description);
							$('#medal_value_'+medal_id).show();
						}
					}
				}

				// Only functions defined in this object can be called
				if (this[event] && typeof(this[event]) == 'function') {
					this[event](params);
				}

			}, 10, jQuery);
		}
		
		var ApiController = (function ($) {
			return function() {

				var controller = this;

				this.getURL = function(url) {
					$("#api_features").load(url, function() {
						controller.initialize();
					});
				};

				var loadFromClick = function(e) {
					if (this.href && this.href.length) {
						controller.getURL(this.href);
					}

					return false;
				};

				this.initialize = function() {
					// these are the buttons in the header of the api items
					// pod, which can be found in portal entries like
					// https://www.newgrounds.com/portal/view/610807
					$("#api_buttons a").click(loadFromClick);

					$("#api_feature a").click(loadFromClick);

					$("#api_user_filter a").click(loadFromClick);

					$("#api_pagination a").click(loadFromClick);

					$("#usergenerated_icons a[name^='loadfile_']")
						.click(function(e) {
							e.preventDefault();
							var save_id = $(this).attr('id');
							if (save_id) {
								var save_info = save_id.split("_");
								var group_id = parseInt(save_info[1]);
								var file_id = parseInt(save_info[3]);
								var href = $(this).attr('href');
								window.postMessage('{event:"load_save_file",href:"'+href+'",group:'+group_id+',file:'+file_id+'}','*');
							}
							return false;
						});

					$('#usergenerated_group') // deprecated, only used by old flash shit
						.change(function(e) {
							var baseURL = document.getElementById('submission_url').value;
							baseURL = baseURL.replace("/portal/view/","/ngio/api_items/");
							controller.getURL(baseURL+'/usergenerated/browse/'+this.value);
						});

					$('#scoreboard_select')
						.change(function(e) {
							controller.getURL($(this).val());
						});

					$('#scoreboard-user-lookup')
						.submit(function(e) {
							e.preventDefault();
							controller.getURL($(this).attr('action')+"?username="+$("input[name='username'").first().val());
						});

					$('#medal_group, #medals_sort, #savegroups_sort')
						.off()
						.change(function(e) {
							controller.getURL(this.value);
						});
				}
			}
		})(jQuery);
		var api_controller = new ApiController();
	//]]>
</script>
						
